<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
  package="com.judgelee.vieweventdemo">

  <application
    android:allowBackup="true"
    android:icon="@mipmap/ic_launcher"
    android:label="@string/app_name"
    android:roundIcon="@mipmap/ic_launcher_round"
    android:supportsRtl="true"
    android:theme="@style/AppTheme">
    <activity android:name="com.judgelee.vieweventdemo.MainActivity">
      <intent-filter>
        <action android:name="android.intent.action.MAIN" />

        <category android:name="android.intent.category.LAUNCHER" />
      </intent-filter>
    </activity>
    <activity android:name="com.judgelee.vieweventdemo.ListViewDetachActivity" />
    <activity android:name="com.judgelee.vieweventdemo.DispatchEventActivity" />
    <activity android:name="com.judgelee.vieweventdemo.OverlapViewActivity" />
    <activity android:name="com.judgelee.vieweventdemo.NestedRecyclerViewActivity" />
    <activity android:name="com.judgelee.vieweventdemo.ListItemClickActivity" />
    <activity android:name="com.judgelee.vieweventdemo.DrawerActivity" />
  </application>

</manifest>